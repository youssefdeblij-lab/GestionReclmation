<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Reclamations" table="reclamations">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="date" type="string" column="date" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateresoulution" type="string" column="dateresoulution" length="50" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="ajoutepar" type="integer" column="ajoutepar" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="resolupar" type="integer" column="resolupar" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="commentaire" type="text" column="commentaire" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="integer" column="status" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="idzone" type="integer" column="idzone" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
